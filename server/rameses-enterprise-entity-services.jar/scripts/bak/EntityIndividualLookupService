import com.rameses.common.*import com.rameses.annotations.*class EntityIndividualLookupService{	@PersistenceContext('entity')	def em		@ProxyMethod	public List getList( params ) {		params.searchtext = (params.searchtext? params.searchtext+'%':'%')				def result = em.sqlContext.createNamedQuery('entityindividual:getList')					.setParameters(params)					.setFirstResult( params._start )					.setMaxResults(params._limit)					.resultList 							return result	}}