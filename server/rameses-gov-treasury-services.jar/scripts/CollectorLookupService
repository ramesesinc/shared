import com.rameses.annotations.*;
import com.rameses.util.*;
import java.rmi.server.*;

class CollectorService {

	@PersistenceContext("main")
	def em;

	@ProxyMethod
	public def getColumns( def p ) {
		return [
			[name:"objid", caption:"ID"],
			[name:"name", caption:"Name"],
		];	
	}	

	@ProxyMethod
	public def getList(def o) { 
		
		def list = [
			[objid:'EMN', name:'NAZARENO, ELMO'],
			[objid:'WVF', name:'FLORES, WORGIE'],
			[objid:'JZM', name:'ZAMORA, JESSIE'],
		];
		if(o.excludecollector) {
			list.remove( list.findAll{it.objid == o.excludecollector} );
		}
		return list;
	}


	
}
