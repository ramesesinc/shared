<!-- REQUISITION ISSUE SLIP -->
<workunit>
    
    <invokers>
       <invoker type="formActions" caption="Close" action="_close" shortcut="ctrl C" mnemonic="c" icon="images/toolbars/cancel.png" immediate="true"/>
       <invoker type="formActions" caption="Next" action="process" visibleWhen="#{mode=='initial'}" mnemonic='n' shortcut="ctrl N" icon="images/toolbars/arrow_right.png"/>
       <invoker type="formActions" caption="Save" action="save" visibleWhen="#{mode=='create'}" shortcut="ctrl S" mnemonic="s" icon="images/toolbars/save.png"/>
       <invoker type="formActions" caption="Print" action="print" visibleWhen="#{mode == 'read'}" shortcut="ctrl P" mnemonic="p" />
    </invokers>
    
    <pages>
        <page name="default" template="com.rameses.enterprise.inventory.stock.StockReceiptPage"/>
    </pages>
    
</workunit>