"entity.*|bank|fund" [mode == 'read'] {
    readonly:true;
}

"entity.*|bank|fund" [mode != 'read'] {
    readonly:false;
}

"entity.objid" [mode == 'create'] {
    readonly:false;
}

"entity.objid" [mode != 'create'] {
    readonly:true;
}

"bank.name|bank.branchname" [mode != 'read'] {
    readonly:true;
}
