"entity.*" [mode == 'read'] {
    readonly:true;
}

"entity.*" [mode != 'read'] {
    readonly:false;
}

"selectedItem|searchText" [mode == 'read'] {
    enabled:true;
}

"selectedItem|searchText" [mode != 'read'] {
    enabled:false;
}

"entity.name" [mode == 'create'] {
    readonly:false;
}

"entity.name" [mode != 'create'] {
    readonly:true;
}
