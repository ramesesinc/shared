<workunit>
    <invokers>
        <invoker type="formActions" action="_close" caption="Back" immediate="true" visibleWhen="#{!completed}" 
            icon="images/toolbars/cancel.png" mnemonic="c"/>
        <invoker type="formActions" action="post" caption="Save" visibleWhen="#{!completed}"
            icon="images/toolbars/save.png" shortcut="ctrl S" />
        
        <invoker type="formActions" action="_close:_close" caption="Quit" visibleWhen="#{completed}" 
            shortcut="ctrl Q"/>
            
        <invoker type="formActions" action="_close" caption="New" visibleWhen="#{completed}" shortcut="ctrl N"/>
            
        <invoker type="formActions" action="reprint" caption="Reprint" 
            visibleWhen="#{completed}" shortcut="ctrl P"/>
            
        <invoker type="formActions" action="doVoid" immediate="true" caption="Void" visibleWhen="#{completed &amp;&amp; !entity.voided}" />
        
        <!-- 
        FOR TESTING VIEWER
        <invoker type="formActions" action="_completed" caption="Test View" immediate="true"/>
        <invoker type="formActions" action="_default" caption="Backi" immediate="true"/>
        -->
    </invokers>
    
    <code class="com.rameses.enterprise.treasury.controllers.BasicCashReceipt"/>

    <pages>
        <page name="completed" template="com.rameses.enterprise.treasury.cashreceipt.CashReceiptInfoPage"/>
    </pages>
    
</workunit>